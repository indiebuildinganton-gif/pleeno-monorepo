{
  "name": "@pleeno/database",
  "version": "0.1.0",
  "private": true,
  "description": "Supabase client and database types for Pleeno",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./server": "./src/server.ts",
    "./client": "./src/client.ts",
    "./middleware": "./src/middleware.ts",
    "./api-route": "./src/api-route.ts",
    "./types": "./src/types/database.types.ts"
  },
  "scripts": {
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint \"src/**/*.{ts,tsx}\" --max-warnings 0 || true",
    "type-check": "tsc --noEmit",
    "gen:types": "supabase gen types typescript --local > src/types/database.types.ts"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "next": "^16.0.2",
    "typescript": "^5.7.2"
  }
}
